    <%= image_tag "https://static1.squarespace.com/static/521be5abe4b0d492d86da9f8/t/54ea9169e4b0a4e937b05ac9/1424658806699/" %>
    <%= image_tag "50b170e0c6a3284d31fe73a1bc1af402.jpg" %>
    <%= image_tag "https://d1zpvjny0s6omk.cloudfront.net/media/cache/92/23/9223d6689ed4a3f5cc15fe77e35e1597.jpg" %>
    <%= image_tag "https://weddinginvitationstalk.files.wordpress.com/2015/04/westin-wedding-ceremony-by.jpg" %>
