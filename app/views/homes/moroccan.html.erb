   <%= image_tag "https://i0.wp.com/somoroccan.com/wp-content/uploads/2015/08/rahal1.jpg" %>
   <%= image_tag "https://images.shaadisaga.com/shaadisaga_production/photos/pictures/000/206/072/new_medium/IMG_0251_20170709172205035.jpg?1502470876" %>
  </figure>
