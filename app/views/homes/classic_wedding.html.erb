  <%= image_tag "DSC_3430.JPG" %>
  <%= image_tag "images(11).jpg" %>
  <%= image_tag "Mandap-Decor.jpg" %>
  <%= image_tag "IMG-20160629-WA0002 - Copy - Copy.jpg" %>
